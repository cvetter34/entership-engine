@font-face
  font-family Gotham Rounded Light
  src url(/assets/gothamrounded-light.eot)
  src url(/assets/gothamrounded-light.eot?#iefix) format(embedded-opentype),
    url(/assets/gothamrounded-light.woff) format(woff),
    url(/assets/gothamrounded-light.ttf) format(truetype),
    url(/assets/gothamrounded-light.svg#gotham_roundedlight) format(svg)
  font-weight normal
  font-style normal

